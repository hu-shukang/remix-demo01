{
  "name": "amplify-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:fastify": "tsc --project ./tsconfig.server.json",
    "build": "remix vite:build && npm run build:fastify",
    "dev": "cross-env ENV=local tsx --watch-path ./server/index.ts ./server/index.ts",
    "dev:main": "cross-env ENV=main tsx --watch-path ./server/index.ts ./server/index.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=local node ./server/index.js",
    "start:main": "cross-env NODE_ENV=main node ./server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.2.0",
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.1",
    "@mcansh/remix-fastify": "^3.4.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "aws-amplify": "^6.5.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.1.1",
    "@aws-amplify/backend-cli": "^1.2.4",
    "@remix-run/dev": "^2.12.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "aws-cdk": "^2.138.0",
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.8"
  }
}
